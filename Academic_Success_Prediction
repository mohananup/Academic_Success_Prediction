odel = LogisticRegression(class_weight=class_weights, max_iter=1000)\nmeta_model = LogisticRegression(max_iter=1000)\n\n\n# Create the stacking ensemble\nstacking_model = StackingClassifier(\n    #estimators=[('xgb', xgb_model), ('lgbm', lgbm_model), ('rf',rf_model)],\n    estimators=[('xgb', xgb_model), ('lgbm', lgbm_model)],\n    final_estimator=meta_model,\n    cv=5  # 5-fold cross-validation for creating meta-features\n)\n\n\n# Create a pipeline with scaling\nstacking_pipeline = Pipeline([\n    ('scaler', StandardScaler()),\n    ('stacking', stacking_model)\n])\n\n# Fit the stacking ensemble\nstacking_pipeline.fit(X_train, y_train)\n\n# Make predictions\ny_pred = stacking_pipeline.predict(X_test)\n\n# Evaluate the model\nfrom sklearn.metrics import accuracy_score, classification_report\n\naccuracy = accuracy_score(y_test, y_pred)\nprint(f\"voting Ensemble Accuracy: {accuracy:.4f}\")\n\nprint(\"\\nClassification Report:\")\nprint(classification_report(y_test, y_pred))\n\n","metadata":{"execution":{"iopub.status.busy":"2024-06-28T13:27:18.011008Z","iopub.execute_input":"2024-06-28T13:27:18.011467Z"},"trusted":true},"execution_count":null,"outputs":[]},{"cell_type":"raw","source":"# Compare with individual models using cross-validation\nmodels = {\n    'XGBoost': Pipeline([('scaler', StandardScaler()), ('model', xgb_model)]),\n    'LightGBM': Pipeline([('scaler', StandardScaler()), ('model', lgbm_model)]),\n    'Stacking Ensemble': stacking_pipeline\n}\n\nfor name, model in models.items():\n    scores = cross_val_score(model, X_train, y_train, cv=5, scoring='accuracy')\n    print(f\"{name} Cross-Validation Accuracy: {scores.mean():.4f} (+/- {scores.std() * 2:.4f})\")","metadata":{}},{"cell_type":"raw","source":"xgb_feature_importances = stacking_pipeline.named_steps['stacking'].named_estimators_['xgb'].feature_importances_\nlgbm_feature_importances = stacking_pipeline.named_steps['stacking'].named_estimators_['lgbm'].feature_importances_\n\n# Get feature importance